<Window x:Class="PL.ManagerInterface.ManagerInterfaceMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL.ManagerInterface"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <StackPanel Grid.Row="1" Grid.RowSpan="2" Grid.Column="2">
            <Button Content="Init"
                Margin="10,10,10,10"
                MinHeight="50"
                Click="Button_Init"/>
            <Button Content="Reset"
                Margin="10,10,10,10"
                MinHeight="50"
                Click="Button_Reset"/>  
            <Button Content="Enginners"
                Margin="10,10,10,10"
                MinHeight="50"
                Click="Button_Engineers"/>
            <Button Content="auto-schedule"
                 Margin="10,10,10,10"
                 MinHeight="50"
                    Click="Button_Auto_Schedule"/>

        </StackPanel>
    </Grid>
</Window>
